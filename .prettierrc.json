{
	"semi": true,
	"trailingComma": "all",
	"singleQuote": true,
	"printWidth": 110,
	"useTabs": true,
	"tabWidth": 4,
	"endOfLine": "auto",
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"importOrder": [
		"^react$",
		"^@?\\w",
		"^(@/)(.*)",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
